<!DOCTYPE fontscolors PUBLIC
    "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN"
    "https://netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="character" default="char"/>
    <fontcolor name="errors" default="error"/>
    <fontcolor name="identifier" default="identifier" foreColor="magenta">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="keyword" default="keyword" foreColor="gray">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="literal" default="literal" />
    <fontcolor name="comment" default="comment"/>
    <fontcolor name="number" default="number" foreColor="orange"/>
    <fontcolor name="operator" default="operator" foreColor="orange"/>
    <fontcolor name="string" default="string"/>
    <fontcolor name="separator" default="separator"/>
    <fontcolor name="whitespace" default="whitespace"/>
    <fontcolor name="instruction" default="method" foreColor="blue">
        <font style="bold" />
    </fontcolor>
</fontscolors>