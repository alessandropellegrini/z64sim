<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Actions">
        <folder name="Run">
            <file name="org-z64sim-editor-AssembleAction.instance_hidden"/>
        </folder>
    </folder>
    <folder name="Editors">
        <folder name="text">
            <folder name="z64asm">
                <attr name="displayName" bundlevalue="org.z64sim.editor.Bundle#Editors/text/z64asm"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.z64sim.editor.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="z64asm" url="z64Template.z64"/>
            </folder>
        </folder>
    </folder>

</filesystem>
